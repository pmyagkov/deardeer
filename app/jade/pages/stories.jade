.page.page_stories
    ul.stories
        - for(var i = 0; i < photos.length; i++)
            li.story(class= i == 0 ? "current" : "")
                .story-inner
                    .story__image
    .arrows.arrows_photo
        .up.change
        .down.change
            .down-inner

    .arrows.arrows_thumbs
        .up.change
        .down.change
            .down-inner

    ul.thumbs.out
        - for(var i = 0; i < photos.length; i++)
            li.thumb(class= i == 0 ? "current" : "", data-id= i)
